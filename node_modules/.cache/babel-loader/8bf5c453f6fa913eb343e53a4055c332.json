EAAA;AACA;;AACA,aAAA,eAAA,KAAA,SAAA,CAAA,IAAA,GACA,SADA,GACA,KAAA,gBADA,GACA,GADA;AAEA,KAvBA;AAwBA,IAAA,gBAxBA,8BAwBA;AACA,UAAA,CAAA,GAAA,OAAA,KAAA,SAAA,CAAA,OAAA;AACA,aAAA,CAAA,CAAA,MAAA,CAAA,CAAA,CAAA,MAAA,GAAA,CAAA,CAAA;AACA,KA3BA;AA4BA,IAAA,eA5BA,6BA4BA;AACA,aAAA,KAAA,SAAA,CAAA,WAAA,GAAA,KAAA,SAAA,CAAA,cAAA;AACA;AA9BA,IAvBA;AAuDA,EAAA,OAAA,oBACA,YAAA,CAAA;AACA,IAAA,qBAAA,EAAA,KAAA,CAAA;AADA,GAAA,CADA;AAvDA,CAAA","sourcesContent":["<template>\r\n    <b-modal v-model=\"mostrarModal\"\r\n             class=\"estilo-modal-municipio\"\r\n             :title=\"titulo\"\r\n             hide-footer header-bg-variant=\"dark\"\r\n             header-text-variant=\"light\"\r\n             size=\"lg\">\r\n\r\n        <div class=\"secao\">Situação</div>\r\n\r\n        <item-info-modal>\r\n            Progresso: {{municipio.percRealizado}}% ({{itensRealizados}} de {{municipio.itensTotais}})\r\n            <b-progress class=\"barra-progresso\" :max=municipio.itensTotais>\r\n                <b-progress-bar\r\n                        :value=\"itensRealizados\"\r\n                        variant=\"success\"\r\n                        animated>\r\n                </b-progress-bar>\r\n            </b-progress>\r\n        </item-info-modal>\r\n\r\n        <item-info-modal>\r\n            Andamento {{ municipio.andamento }} - {{ descAndamento(municipio.andamento) }}\r\n        </item-info-modal>\r\n\r\n        <item-info-modal>\r\n            Período de realização: {{ municipio.periodoEtapa }}\r\n        </item-info-modal>\r\n\r\n        <div v-if=\"municipio.pontoTr\">\r\n            <div class=\"secao\">Transmissão remota</div>\r\n            <item-info-modal>\r\n                {{ municipio.pontoTr.numero}} - {{municipio.pontoTr.descricao}}\r\n            </item-info-modal>\r\n        </div>\r\n\r\n        <div class=\"secao\">Suporte Tecnológico</div>\r\n        <b-table :items=\"municipio.sts\" small thead-class=\"sem-cabecalho\"></b-table>\r\n\r\n    </b-modal>\r\n</template>\r\n\r\n<script>\r\n    import ItemInfoModal from \"./ModalInfoItem\";\r\n    import {mapMutations, mapState, mapGetters} from 'vuex'\r\n    import * as tipos from '../store/mutationtypes'\r\n\r\n    export default {\r\n        name: 'VisaoMunicipiosModal',\r\n        components: {ItemInfoModal},\r\n        props: {\r\n            municipio: {\r\n                codIbge: 2600054,\r\n                andamento: 1,\r\n                situacaoVerificacao: 'ATIVO',\r\n                dataUltimaAtualizacao: '',\r\n                percRealizado: 0,\r\n                itensRestantes: 1,\r\n                itensTotais: 1,\r\n                sts: [],\r\n                sedePtr: false,\r\n                pontoTr: {\r\n                    numero: 3,\r\n                    descricao: '',\r\n                    latitude: '',\r\n                    longitude: '',\r\n                    tipo: 'PAT'\r\n                }\r\n            }\r\n        },\r\n        computed: {\r\n            ...mapState({\r\n                _mostrarModal: state => state.mostrarModalMunicipio,\r\n                municipios: state => state.municipios\r\n            }),\r\n            ...mapGetters({\r\n                andamentos: 'getAndamentos',\r\n                descAndamento: 'getDescAndamento'\r\n            }),\r\n            mostrarModal: {\r\n                get() {\r\n                    return this._mostrarModal;\r\n                },\r\n                set(valor) {\r\n                    this.atualizarMostrarModal(valor);\r\n                }\r\n            },\r\n            titulo() {\r\n                if (!this.municipio) {\r\n                    return ''\r\n                }\r\n                return \"Município \" + this.municipio.nome\r\n                    + \" (Polo \" + this.numPoloFormatado + \")\";\r\n            },\r\n            numPoloFormatado() {\r\n                const s = \"00\" + this.municipio.numPolo;\r\n                return s.substr(s.length - 2);\r\n            },\r\n            itensRealizados() {\r\n                return this.municipio.itensTotais - this.municipio.itensRestantes\r\n            }\r\n        },\r\n        methods: {\r\n            ...mapMutations({\r\n                atualizarMostrarModal: tipos.MOSTRAR_MODAL_MUNICIPIO\r\n            })\r\n        }\r\n    }\r\n</script>\r\n\r\n<style>\r\n    .estilo-modal-municipio {\r\n        font-family: \"Roboto Condensed\", sans-serif;\r\n    }\r\n\r\n    .secao {\r\n        font-weight: bold;\r\n        margin-top: 7px;\r\n    }\r\n\r\n    .barra-progresso {\r\n        height: 25px;\r\n    }\r\n</style>"],"sourceRoot":"src/componentes"}]}